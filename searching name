#include<stdio.h>
int main()
{
   char s1[20][20],name[20];
   	int i,n,flag=1;
	printf("enter limit");
	scanf("%d",&n);
    printf("enter n student names");
	   for(i=0;i<n;i++)
	   {
	   scanf("%s",&s1[i]);
       }
  printf("enter student name to serch");
  scanf("%s",&name);
	   for(i=0;i<n;i++)
	 {
	    	if(strcmp(s1[i],name)==0)	
			{			 
			    flag=0; 	    	
	 		    break;
	 	   }
     }
		 if(flag==0)
 printf("name is found");
else
  printf("name is not found");	
}
